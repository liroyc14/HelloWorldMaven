pipeline { 
    agent any 
    stages {
        stage('Build') { 
            tools {
                maven 'maven3.3.9'
            }
            steps {
                sh "mvn clean compile"
            }
        }
        stage('Test'){ 
            tools {
                maven 'maven3.3.9'
            }
            steps {
                sh "mvn test"
            }
        }
    }
}